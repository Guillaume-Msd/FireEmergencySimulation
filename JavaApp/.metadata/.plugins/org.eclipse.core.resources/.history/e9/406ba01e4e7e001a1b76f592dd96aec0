package emergency;

public class Vehicule_pompiers extends AbstractVehicule {
	
	private Integer InterventionSpeed;
	
	private Integer LiquidCapacity;
	
	private Integer InterventionOilConsumption;  //En L/100km
	
	private Integer NormalOilConsumption;  //En L/100km

	
	public Vehicule_pompiers() {
		this.InterventionSpeed = 70; // 70 km/h en ville ne semble pas aberrant
		this.NormalSpeed = 40;
		this.OilCapacity = 400;
		this.LiquidCapacity = 10000;
		this.InterventionOilConsumption = 30;
		this.NormalOilConsumption = 20;
		this.setEtat(EnumEtat.Neuf);
	}
}