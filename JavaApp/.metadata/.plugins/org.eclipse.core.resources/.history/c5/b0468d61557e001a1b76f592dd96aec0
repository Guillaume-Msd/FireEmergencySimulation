package simulator;

import java.util.HashMap;
import java.util.List;
import java.util.Map;

import emergency.CarburantEnum;

public class StationService extends EnvironmentElement {
	private Map<CarburantEnum,Double> carburants;
	public Map<CarburantEnum,Double> capacites_max;
	
	public StationService(Map<CarburantEnum,Double> capacites_max) {
		this.capacites_max = capacites_max;
		this.setCarburants(capacites_max);
	}	


	public static void main(String[] args) {
		Map<CarburantEnum,Double> capas = new HashMap();
		capas.put(CarburantEnum.Gazole, 10000.0);
		capas.put(CarburantEnum.E85, 200000.0);
		StationService s = new StationService(capas);
		capas.put(CarburantEnum.GLPc,5.0);
		System.out.println(s.getCarburants());
		System.out.println(s.capacites_max);
	}

	/**
	 * @return the carburants
	 */
	public Map<CarburantEnum,Double> getCarburants() {
		return carburants;
	}


	/**
	 * @param carburants the carburants to set
	 */
	public void setCarburants(Map<CarburantEnum,Double> carburants) {
		this.carburants = carburants;
	}
}